<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ie.ucd.larryoshea.practical4" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ie.ucd.larryoshea.practical4.MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ie.ucd.larryoshea.practical4.DisplayModule"
            android:label="DisplayName" >
            <intent-filter>
                <action android:name="ie.ucd.larryoshea.practical4.DisplayModule" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="ie.ucd.larryoshea.practical4.prefsActivity"
            android:label="prefsActivity" >
            <intent-filter>
                <action android:name="ie.ucd.larryoshea.practical4.prefsActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
